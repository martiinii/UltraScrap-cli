{
  "name": "ultrastar",
  "description": "Download songs from UltraStar biggest database",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "keywords": [
    "ultrastar",
    "cli",
    "download",
    "songs"
  ],
  "repository": {
