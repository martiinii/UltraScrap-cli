  },
  "dependencies": {
    "effect": "^3.17.7",
    "env-paths": "^3.0.0",
    "ink": "^6.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.1"
